
public class Var01 {

	public static void main(String[] args) {
		
		// 문자열(String) 저장 변수
		// str : 변수(이름은 마음대로 작성 가능)
		// 변수명은 대, 소문자 구분
		String str = "홍길동";  // 이름
		String STR = "홍길동";
		
		// 변수명을 직관적으로 작성
		String name = "홍길동";
		
		// 문자열 변수 선언
		String name1;
		// 변수 재사용 가능
		name1 = "홍길동";
		name1 = "박길동";
		
		System.out.println(str + "\n" + STR);
		
		
		
		
	}

}
