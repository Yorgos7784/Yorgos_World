
public class Var1_parctice {

	public static void main(String[] args) {
		
		String name = "홍길동";
		int age = 100;
		char se = '남';
		float weight = 60.5f;
		double height = 178.6;
		String adr = "조선 한양 홍대감댁";
		
		System.out.println("     개인 신상 정보");
		System.out.println("-----------------------");
		System.out.println("이름 : " + name);
		System.out.println("-----------------------");
		System.out.println("나이 : " + age + "살");
		System.out.println("-----------------------");
		System.out.println("성별 : " + se);
		System.out.println("-----------------------");
		System.out.println("신장 : " + height + "cm");
		System.out.println("-----------------------");
		System.out.println("체중 : " + weight + "kg");
		System.out.println("-----------------------");
		System.out.println("주소 : " + adr);
		
		
		

	}

}
